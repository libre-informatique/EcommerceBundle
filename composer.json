{
    "name": "libre-informatique/ecommerce-bundle",
    "type": "symfony-bundle",
    "description": "Bridge bundle between librinfo bundles and sylius",
    "minimum-stability": "dev",
    "require": {
        "php": ">= 7.1",
        "symfony/symfony": ">=3.3",
        "blast-project/core-bundle": "self.version",
        "blast-project/base-entities-bundle": "self.version",
        "blast-project/utils-bundle": "self.version",
        "blast-project/outer-extension-bundle": "self.version",
        "sylius/sylius": "^1.0",
        "libre-informatique/sonata-sylius-user-bundle": "self.version",
        "libre-informatique/crm-bundle": "self.version",
        "knplabs/knp-snappy-bundle": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": ">=3.2"
    },
    "config": {
        "sllh-composer-lint": {
            "php": true,
            "type": true,
            "minimum-stability": false,
            "version-constraints": true
        }
    },
    "license": "LGPL-3.0",
    "keywords": [
        "e-commerce",
        "libio",
        "sonata",
        "symfony"
    ],
    "homepage": "https://github.com/libre-informatique/SymfonyLibrinfoEcommerceBundle",
    "authors": [
        {
            "name": "Marcos Bezerra de Menezes",
            "email": "marcos.bezerra@libre-informatique.fr"
        },
        {
            "name": "Libre Informatique",
            "homepage": "http://www.libre-informatique.fr/"
        }
    ],
    "version": "dev-wip-lisem",
    "autoload": {
        "psr-4": {
            "Librinfo\\EcommerceBundle\\": "src/"
        }
    }
}
